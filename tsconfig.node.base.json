{
  "compilerOptions": {
    "lib": [
      "es2023"
    ],
    "types": [
      "node"
    ],
    "module": "ESNext",
    "target": "es2022",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "composite": true,
    "checkJs": true,
    "allowJs": true,
    "noEmit": true,
    "resolveJsonModule": true
  }
}
