{
  "name": "@petr-ptacek/vue-core",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:types": "vue-tsc -b ./tsconfig.types.json",
    "type-check": "vue-tsc -b ./tsconfig.app.json",
    "type-watch": "vue-tsc -b ./tsconfig.app.json --watch",
    "test": "vitest",
    "coverage": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/petr-ptacek/js-lab",
    "directory": "packages/vue-ui"
  },
  "homepage": "https://github.com/petr-ptacek/ja-lab",
  "files": [
    "dist"
  ],
  "types": "./dist/types/index.d.ts",
  "style": "./dist/style.css",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js"
    },
    "./style.css": {
      "default": "./dist/style.css"
    }
  },
  "sideEffects": [
    "*.css"
  ],
  "dependencies": {
    "@petr-ptacek/js-core": "workspace:*",
    "@petr-ptacek/vue-test-utils": "workspace:*",
    "@vueuse/core": "^14.0.0"
  },
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "jsdom": "^27.4.0",
    "vite": "^7.3.1",
    "vue": "^3.5.26",
    "vue-tsc": "^3.1.4"
  },
  "engines": {
    "node": ">=22"
  }
}
