{
  "name": "@petr-ptacek/js-core",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build:types": "tsc -b ./tsconfig.types.json",
    "type-check": "tsc -b ./tsconfig.app.json",
    "type-watch": "tsc -b ./tsconfig.app.json --watch",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/petr-ptacek/js-lab.git",
    "directory": "packages/js-core"
  },
  "homepage": "https://github.com/petr-ptacek/js-lab",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "devDependencies": {
    "vite": "^7.3.1"
  },
  "engines": {
    "node": ">=22"
  }
}
