{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "src/index.ts"
  ],
  "out": "docs/api-generated",
  "plugin": [
    "typedoc-plugin-markdown"
  ],
  "readme": "none",
  "disableSources": true,
  "excludePrivate": true,
  "excludeInternal": true,
  "excludeProtected": true,
  "categorizeByGroup": false,
  "sort": [
    "source-order"
  ],
  "hideBreadcrumbs": true,
  "hidePageHeader": true,
  "cleanOutputDir": true
}
